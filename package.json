{
  "name": "frontend-base",
  "version": "0.1.0",
  "private": true,
  "author": "Marko Savic",
  "keywords": [
    "tea-effect",
    "effect",
    "typescript",
    "react",
    "elm-architecture"
  ],
  "description": "Base project for admin/dashboard applications using tea-effect architecture.",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.config.js --env env=dev",
    "build": "webpack --config webpack/webpack.config.js --env env=prod",
    "checkts": "tsc --noEmit",
    "lint": "eslint ./src",
    "prettier": "prettier --list-different \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "fix-lint": "eslint --fix ./src",
    "fix-prettier": "prettier --write \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test": "yarn prettier && yarn checkts && yarn test:unit",
    "prepare": "husky"
  },
  "dependencies": {
    "@effect/platform": "^0.94.0",
    "effect": "^3.19.15",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tea-effect": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@effect/vitest": "^0.27.0",
    "@fluentui/react-components": "^9.56.0",
    "@fluentui/react-icons": "^2.0.318",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.2.0",
    "css-loader": "^7.1.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-import-resolver-typescript": "^4.4.0",
    "eslint-plugin-import-x": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.0",
    "lint-staged": "^15.4.0",
    "prettier": "^3.4.0",
    "react-refresh": "^0.14.0",
    "style-loader": "^4.0.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.22.0",
    "vitest": "^3.0.0",
    "webpack": "^5.97.0",
    "webpack-cli": "^6.0.0",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
